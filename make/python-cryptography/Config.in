config FREETZ_PACKAGE_PYTHON_CRYPTOGRAPHY
	bool "cryptography 1.5.2"
	depends on FREETZ_PACKAGE_PYTHON
	select FREETZ_PACKAGE_PYTHON_SIX
	select FREETZ_PACKAGE_PYTHON_PYASN1
	select FREETZ_PACKAGE_PYTHON_SETUPTOOLS
	select FREETZ_PACKAGE_PYTHON_CFFI
	select FREETZ_PACKAGE_PYTHON_ENUM34
	select FREETZ_PACKAGE_PYTHON_IPADDRESS
	select FREETZ_PACKAGE_PYTHON_IDNA
	select FREETZ_PACKAGE_PYTHON_MOD_SSL
	default n
	help
		cryptography is a package which provides 
		cryptographic recipes and primitives to 
		Python developers.
